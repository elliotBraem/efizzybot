# Twitter API Credentials
TWITTER_USERNAME=your_twitter_username
TWITTER_PASSWORD=your_twitter_password
TWITTER_EMAIL=your_twitter_email

# Environment
NODE_ENV=development

# Telegram Export Configuration
TELEGRAM_ENABLED=false
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHANNEL_ID=your_channel_id

# RSS Export Configuration
RSS_ENABLED=false
RSS_TITLE=Public Goods News
RSS_DESCRIPTION=Latest approved public goods submissions
RSS_FEED_PATH=public/feed.xml
RSS_MAX_ITEMS=100

# NEAR Export Configuration
NEAR_ENABLED=false
NEAR_NETWORK_ID=testnet
NEAR_NODE_URL=https://rpc.testnet.near.org
NEAR_CONTRACT_ID=your_contract_id.testnet
NEAR_ACCOUNT_ID=your_account_id.testnet
NEAR_PRIVATE_KEY=your_private_key
